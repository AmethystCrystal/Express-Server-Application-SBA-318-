<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>User Details</title>
    <link rel="stylesheet" href="/style.css"> 
</head>
<body>
   

<div>
    <% users.forEach(function(user) { %>
      <li id="hi"><%= user.id  %></li>
      <li><%= user.name  %></li>
      <li><%= user.username  %></li>
      <li><%= user.email  %></li>
    <% }); %>
    </div>
  
</body>
</html>
